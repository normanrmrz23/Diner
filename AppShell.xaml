<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="Diner.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:Diner.Views"
    Shell.FlyoutBehavior="Disabled">

    <TabBar>
        <ShellContent
            ContentTemplate="{DataTemplate views:HomePage}"
            Icon="house_solid.png"/>
        <ShellContent
            ContentTemplate="{DataTemplate views:FeedPage}"
            Icon="utensils_solid.png" />
        <ShellContent
            ContentTemplate="{DataTemplate views:SearchPage}"
            Icon="magnifying_glass_solid"/>
        <ShellContent
            ContentTemplate="{DataTemplate views:AllListsPage}"
            Icon="list_solid.png" />
        <ShellContent
            ContentTemplate="{DataTemplate views:ProfilePage}"
            Icon="user_solid.png" />
    </TabBar>

</Shell>